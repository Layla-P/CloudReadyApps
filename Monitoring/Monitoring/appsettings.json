{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Management": {
    "Tracing": {
      "AlwaysSample": true
    },
    "endpoints": {
      "actuator": {
        "exposure": {
          "include": [ "*" ]
        }
      }
    },
    "metrics": {
      "observer": {
        "EventCounterEvents": "true",
        "GCEvents": "true",
        "ThreadPoolEvents": "true"
      }
    }
  },
  "AllowedHosts": "*"
}
